{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
        "ticket_id": {
            "type": "number",
            "minLength": 1,
            "minimum": 0
        },
        "username": {
            "type": "string",
            "minLength": 5,
            "maxLength": 30
        },
        "status": {
            "type": "string",
            "enum": ["Approved", "Denied"]
        }
    },
    "additionalProperties": false,
    "required": [
        "ticket_id",
        "username",
        "status"
    ]
}